{
  "name": "nff-cloud",
  "version": "1.0.62",
  "description": "Netoffice dashboard for AWS Workspace",
  "author": "Ariel Menendez",
  "main": "./electron/dist/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "ng build --base-href \"./\" && tsc -p electron && electron .",
    "electronDeploy": "ng build --prod --base-href \"./\" && tsc -p electron",
    "publish": "npm run electronDeploy && electron-builder build --win -p always"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.1.3",
    "@angular/common": "~10.1.3",
    "@angular/compiler": "~10.1.3",
    "@angular/core": "~10.1.3",
    "@angular/forms": "~10.1.3",
    "@angular/platform-browser": "~10.1.3",
    "@angular/platform-browser-dynamic": "~10.1.3",
    "@angular/router": "~10.1.3",
    "@fortawesome/angular-fontawesome": "^0.7.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@types/express": "^4.17.8",
    "activedirectory": "^0.7.2",
    "aws-sdk": "^2.771.0",
    "axios": "^0.20.0",
    "bootstrap": "^4.5.2",
    "buffer": "^5.6.0",
    "electron-log": "^4.2.4",
    "electron-updater": "^4.3.5",
    "hammerjs": "^2.0.8",
    "httpntlm": "^1.7.6",
    "jquery": "^3.5.1",
    "net-user": "^1.1.0",
    "ngx-electron": "^2.2.0",
    "passport-windowsauth": "^3.0.1",
    "popper.js": "^1.16.1",
    "react-xml-editor": "^0.6.0",
    "rxjs": "~6.6.0",
    "taskkill": "^3.1.0",
    "tslib": "^2.0.0",
    "underscore": "^1.11.0",
    "username": "^5.1.0",
    "win-tasklist": "^1.0.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.3",
    "@angular/cli": "~10.1.3",
    "@angular/compiler-cli": "~10.1.3",
    "@types/electron": "^1.6.10",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/underscore": "^1.10.24",
    "codelyzer": "^6.0.0",
    "electron": "^10.1.3",
    "electron-builder": "^22.8.1",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  },
  "build": {
    "appId": "com.github.menendea.nffcloud",
    "files": [
      "dist/nffCloud/*",
      "electron/dist/main.js",
      "electron/dist/renderer.js",
      "electron/dist/netuser.js",
      "electron/dist/downloadconfigs.js",
      "assets/img/*"
    ],
    "mac": {
      "category": "nffcloud.app.category.type",
      "target": [
        "zip",
        "dmg"
      ]
    }
  }
}
